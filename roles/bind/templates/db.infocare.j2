; Zone file for {{ startup_name }}
$TTL    604800
@       IN      SOA     ns.{{ startup_name }}. admin.{{ startup_name }}. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.{{ startup_name }}.
ns      IN      A       {{ bind_server_ip }}
